  "weights": {
    "pages_minus_refs": 0.3,
    "references_count": 0.25,
    "recency_delay_years": 0.2,
    "cites_per_day": 0.15,
    "mean_author_hindex": 0.1
  },
  "q05": 0.05,
  "q95": 0.95
}