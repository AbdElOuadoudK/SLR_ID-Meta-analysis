{
  "phase": "Phase 1 \u2014 Extractor Only (CSV-only; Sheets auto-fills computations; CONFIG sheet)",
  "slr_id": "DEV",
  "s2": {
    "base_url": "https://api.semanticscholar.org",
    "paper_batch_fields": "paperId,title,abstract,year,publicationDate,referenceCount,citationCount,externalIds,isOpenAccess,openAccessPdf,journal,authors.authorId",
    "author_batch_fields": "authorId,name,hIndex",
    "references_fields": "citedPaper.year",
