{
  "name": "Codespace with GROBID (lightweight)",
  "dockerComposeFile": "docker-compose.yml",
  "service": "workspace",
  "workspaceFolder": "/workspace",
  "shutdownAction": "none",
  "forwardPorts": [8070, 8071],
  "postCreateCommand": "pip install -r /workspace/requirements.txt || true",
  "remoteUser": "vscode"
}
